<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Feleves_feladat"
             x:Class="Feleves_feladat.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="PrimaryColor">#FFD5D5</Color>
            <Color x:Key="SecondaryColor">#FDAAAA</Color>
            <Color x:Key="ThirdColor">#59AC77</Color>
            <Color x:Key="FourthColor">#3A6F43</Color>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
